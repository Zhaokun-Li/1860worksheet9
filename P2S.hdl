CHIP P2S {
    IN indata[4], enable, load;
    OUT sout, complete;

    PARTS:
    Register(in=indata, load=load, out=storedData);
    Counter(width=2, inc=enable, out=bitCounter);
    Mux4Way16(
        a[0..15]=0, b[0..15]=0, c[0..15]=0, d[0..15]=storedData,
        sel=bitCounter, out=sout
    );
    And(a=enable, b=bitCounter[3], out=complete);
}
