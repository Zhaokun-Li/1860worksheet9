CHIP P2S {
    IN indata[4], enable, load;
    OUT sout, complete;

    PARTS:
    Register(in=indata, load=load, out=storedData);
    Counter(width=2, inc=enable, out=bitCounter);
    Or16(a[0..3]=storedData, a[4..15]=false, out=extendedData);
    Mux4Way16(
        a=false, b=false, c=false, d=extendedData,
        sel=bitCounter, out=sout
    );
    And(a=enable, b=bitCounter[1], b=bitCounter[0], out=complete);
}
